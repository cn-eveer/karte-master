{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "main": "public/electron.js",
  "homepage": "./",
  "author": "eagles+",
  "description": "Karte Master",
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "axios": "^1.1.2",
    "node": "^18.10.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.4.2",
    "react-scripts": "5.0.1",
    "react-window": "^1.8.7",
    "sass": "^1.55.0",
    "web-vitals": "^2.1.4",
    "electron": "^21.1.1",
    "electron-builder": "^23.6.0",
    "electron-is-dev": "^2.0.0"
  },
  "build": {
    "appId": "com.electron.kartemaster",
    "productName": "Karte Master",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "public"
    },
    "mac": {
      "target": "dmg"
    },
    "win": {
      "target": "nsis"
    },
    "linux": {
      "target": "deb"
    }
  },
  "scripts": {
    "start": "react-scripts start",
    "start-electron": "electron .",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "dev": "concurrently -k \"BROWSER=none npm run start\" \"npm:electron\"",
    "electron": "wait-on tcp:3000 && electron .",
    "format": "prettier --check ./src",
    "format:fix": "prettier --write ./src",
    "package": "npm run build && electron-builder build -m -c.extraMetadata.main=build/electron.js --publish never"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@emotion/react": "^11.10.4",
    "@emotion/styled": "^11.10.4",
    "@mui/icons-material": "^5.10.9",
    "@mui/material": "^5.10.9",
    "concurrently": "^7.4.0",
    "electron": "^21.1.1",
    "electron-builder": "^23.6.0",
    "electron-is-dev": "^2.0.0",
    "prettier": "^2.7.1",
    "wait-on": "^6.0.1"
  },
  "extraResources": [
    {
      "from": "./public/assets",
      "to": "assets"
    }
  ]
}
